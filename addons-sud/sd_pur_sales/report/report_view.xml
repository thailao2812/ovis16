<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<!-- Commercial Invoice    -->
		<record id="report_commercial_invoice" model="ir.actions.report">
	        <field name="name">Report Commercial Invoice</field>
	        <field name="type">ir.actions.report</field>
	        <field name="model">shipping.instruction</field>
	        <field name="binding_model_id" ref="sd_pur_sales.model_shipping_instruction"/>
	        <field name="report_name">report_commercial_invoice</field>
	        <field name="report_type">aeroo</field>
	        <field name="in_format">oo-odt</field>
	        <field name="out_format" model="report.mimetypes" search="[('code','=','oo-doc'),('compatible_types','=','oo-odt')]"/>
	        <field name="preview">True</field>
	        <field name="parser_state">loc</field>
	        <field name="parser_loc">sd_pur_sales/report/report_commercial_invoice_parser.py</field>
	        <field name="report_file">sd_pur_sales/report/report_commercial_invoice.odt</field>
	        <field name="tml_source">file</field>
	        <field name="print_report_name">'Commercial Invoice'</field>
	    </record>	      
	    
	    <!-- Trucking_list_report    -->
