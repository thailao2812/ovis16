	        <field name="preview">True</field>
	        <field name="parser_state">loc</field>
	        <field name="parser_loc">sd_pur_sales/report/trucking_list_parser.py</field>
	        <field name="report_file">sd_pur_sales/report/trucking_list.odt</field>
	        <field name="tml_source">file</field>
	        <field name="print_report_name">'Trucking list'</field>
	    </record>	    
	    
	    
