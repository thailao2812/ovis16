<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
       <!-- Ned Packing -->
    	<record id="name_capacity_tree_view" model="ir.ui.view">
            <field name="name">name.capacity.tree.view</field>
            <field name="model">name.capacity</field>
            <field name="arch" type="xml">
                <tree string="Capacity" editable="top">
                    <field name="name"/>
                    <field name="tph"/>
                    <field name="month_capacity"/>
                    <field name="annual_capacity"/>
                </tree>
            </field>
        </record>
        
        
        <record id="name_capacity_action_view" model="ir.actions.act_window">
            <field name="name">Name Capacity Action</field>
            <field name="res_model">name.capacity</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
              		Create a name Capacity, the first step of a Name Capacity.
              </p>
            </field>
        </record>
        
        <!--  Packing -->
    	<record id="available_operation_time_tree_view" model="ir.ui.view">
            <field name="name">available.operation.time.tree.view</field>
            <field name="model">available.operation.time</field>
            <field name="arch" type="xml">
                <tree string="Capacity" editable="top">
                    <field name="name"/>
                    <field name="hours"/>
                    <field name="non_indus_downtime"/>
                </tree>
            </field>
        </record>
        
        
        <record id="available_operation_time_action" model="ir.actions.act_window">
            <field name="name">Available Operation Time</field>
            <field name="res_model">available.operation.time</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
              		Create a Available Opeartion Time, the first step of a Available Opeartion Time.
              </p>
            </field>
        </record>
        
        <record id="available_opeartion_time_tree_view" model="ir.ui.view">
            <field name="name">planed.operation.time.tree.view</field>
            <field name="model">planed.operation.time</field>
            <field name="arch" type="xml">
                <tree string="Capacity" editable="top">
                    <field name="name"/>
                    <field name="operation_hours"/>
                    <field name="scheduled_downtime_hours"/>
                    <field name="non_scheduled_downtime_hours"/>
                </tree>
            </field>
        </record>
        
        
        <record id="planed_operation_time_action" model="ir.actions.act_window">
            <field name="name">Planed Operation Time</field>
            <field name="res_model">planed.operation.time</field>
            <field name="view_mode">tree</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
              		Create a Planed Operation Time, the first step of a Available Operation Time.
              </p>
            </field>
        </record>

    </data>
</odoo>