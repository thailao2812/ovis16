<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for sale.contract --> 
        <!-- NLS -->
        <record id="seq_sale_nls" model="ir.sequence">
            <field name="name">NLS</field>
            <field name="code">sale.nls</field>
            <field name="prefix">NLS-</field>
            <field name="padding">5</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- NVS -->
        <record id="seq_sale_nvs" model="ir.sequence">
            <field name="name">NVS</field>
            <field name="code">sale.nvs</field>
            <field name="prefix">SVNS-</field>
            <field name="padding">5</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_nvs_traffic" model="ir.sequence">
            <field name="name">NVS Traffic</field>
            <field name="code">sale.nvs.traffic</field>
            <field name="prefix">SVNS-</field>
            <field name="padding">6</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- SI -->
        <record id="seq_shipping_instruction" model="ir.sequence">
            <field name="name">SI</field>
            <field name="code">shipping.instruction</field>
            <field name="prefix">SI-</field>
            <field name="padding">5</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- DO -->
        <record id="seq_delivery_order" model="ir.sequence">
            <field name="name">DO</field>
            <field name="code">delivery.order</field>
            <field name="prefix">DO-%(y)s-%(month)s-</field>
            <field name="padding">3</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- TR -->
        <record id="seq_transfer_order" model="ir.sequence">
            <field name="name">DO-Transfer</field>
            <field name="code">delivery.order.transfer</field>
            <field name="prefix">TO-%(y)s-%(month)s-</field>
            <field name="padding">3</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Post Shipment -->
        <record id="seq_post_shipment" model="ir.sequence">
            <field name="name">PS</field>
            <field name="code">post.shipment</field>
            <field name="prefix">PS-%(y)s-%(month)s-</field>
            <field name="padding">5</field>
            <field name="suffix"></field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- daily.confirmation -->
         <record id="seq_daily_confirmation" model="ir.sequence">
            <field name="name">Daily Confirmation</field>
            <field name="code">daily.confirmation</field>
            <field name="prefix">DailyConfirmation-%(year)s/%(month)s/%(day)s-</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</openerp>
