	        <field name="arch" type="xml">
				<tree string="PIC" editable="top">
					<field name="name"/>
					<field name="code"/>
				</tree>
			</field>
		</record>
	
		
		<record model="ir.actions.act_window" id="action_pic_traffic">
            <field name="name">PIC</field>
            <field name="res_model">s.pic</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a sale contract, the first step of a new contract.
                </p><p>
                </p>
            </field>
        </record>
        
        
