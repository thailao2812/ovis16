<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    	
    	<record id="view_ship_month_tree" model="ir.ui.view">
	        <field name="name">view.s.period.tree</field>
	        <field name="model">s.period</field>
	        <field name="arch" type="xml">
				<tree string="Shipt Month" editable="top">
					<field name="name"/>
					<field name="date_from"/>
					<field name="date_to"/>
					<field name="code"/>
				</tree>
			</field>
		</record>


		<record id="action_ship_month_traffic" model="ir.actions.act_window">
            <field name="name">Shipt Month</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">s.period</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a period, the first step of a new Period.
                </p><p>
                </p>
            </field>
        </record>

		
				  
				  
    	
    	<record id="view_shipped_by_tree" model="ir.ui.view">
	        <field name="name">view.s.ship.by.tree</field>
	        <field name="model">s.ship.by</field>
	        <field name="arch" type="xml">
				<tree string="Shipped By" editable="top">
					<field name="name"/>
					<field name="code"/>
				</tree>
			</field>
		</record>


		<record id="action_ship_by_traffic" model="ir.actions.act_window">
            <field name="name">Shipped By</field>
            <field name="res_model">s.ship.by</field>
            <field name="view_mode">tree</field>
             <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a ship, the first step of a new ship.
                </p><p>
                </p>
            </field>
        </record>
        
