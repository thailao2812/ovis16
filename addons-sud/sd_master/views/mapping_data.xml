<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_mapping_product_tree" model="ir.ui.view">
            <field name="name">view.mapping.product.tree</field>
            <field name="model">mapping.product</field>
            <field name="arch" type="xml">
                <tree string="Mapping product" editable ='top'>
                	<field name="grade"/>
                	<field name="screen"/>
					<field name="code"/>
					<field name="quality"/>
					<field name="product_id"/>
	        	</tree>
            </field>
        </record>
		
		<record id="action_mapping_product" model="ir.actions.act_window">
            <field name="name">Mapping Product</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">mapping.product</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_mapping_product_tree"/>
        </record>
        
		
		<record id="view_mapping_partner_tree" model="ir.ui.view">
            <field name="name">view.mapping.partner.tree</field>
            <field name="model">mapping.partner</field>
            <field name="arch" type="xml">
                <tree string="Mapping Partner" editable ='top'>
                	<field name="client_name"/>
                	<field name="mapping"/>
					<field name="patner_id"/>
	        	</tree>
            </field>
        </record>
		
		<record id="action_mapping_partner" model="ir.actions.act_window">
            <field name="name">Mapping Partner</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">mapping.partner</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_mapping_partner_tree"/>
        </record>
        
		
		
    	
	</data>
</odoo>
