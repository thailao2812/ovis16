<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		
        <record id="module_category_traffic_contract" model="ir.module.category">
             <field name="name">Traffic Contract</field>
            <field name="sequence">10</field>
        </record>

	    <record id="group_traffic_user" model="res.groups">
            <field name="name">Traffic Contract User</field>
            <field name="category_id" ref="sd_traffic.module_category_traffic_contract"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin')),(4, ref('base.user_root'))]"/>
        </record>

        <record id="group_traffic_manager" model="res.groups">
            <field name="name">Traffic Contract Manager</field>
            <field name="category_id" ref="sd_traffic.module_category_traffic_contract"/>
            <field name="implied_ids" eval="[(4, ref('group_traffic_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin')),(4, ref('base.user_root'))]"/>
        </record>
		
	</data>
</odoo>