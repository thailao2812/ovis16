<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.ui.view" id="view_sale_contract_tree_allocate_sp_qc_confirm_inherit_sd_traffic">
            <field name="name">view_sale_contract_tree_allocate_sp_qc_confirm_inherit_sd_traffic</field>
            <field name="model">sale.contract</field>
            <field name="inherit_id" ref="sd_sale_contract.view_sale_contract_tree_allocate_sp_qc_confirm"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='request_qty']" position="after">
                    <field name="total_allocate_qty"/>
                </xpath>
                <xpath expr="//field[@name='scertificate_id']" position="after">
                    <field name="pss_type"/>
                    <field name="pss_approved"/>
                    <field name="pss_count_sent"/>
                    <field name="pss_reject"/>
                    
                </xpath>
            </field>
        </record>
</odoo>