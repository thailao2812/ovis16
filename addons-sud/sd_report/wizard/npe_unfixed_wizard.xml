<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <record id="view_npe_unfixed_wizard" model="ir.ui.view">
           <!--  <div class="oe_title">
                <h1> -->
            <field name="name">NPE Unfixed Quality</field>
                <!-- </h1>
            </div> -->
            <field name="model">npe.unfixed.insert</field>
            <field name="arch" type="xml">
                <form string="Unfixed Quality">
                    
