<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">
        <record id="default_api_restfull_access_token_config" model="ir.config_parameter">
            <field name="key">api_restfull.access_token</field>
            <field name="value">access_token</field>
        </record>

        <record id="default_api_restfull_url_config" model="ir.config_parameter">
            <field name="key">api_restfull.url</field>
            <field name="value">url</field>
        </record>

        <record id="api_account_move_except_config" model="ir.config_parameter">
            <field name="key">api_restfull.api_account_move_exception</field>
            <field name="value">[]</field>
        </record>

        <record id="default_cron_runall" model="ir.cron">
<!--            <field name="user_id" ref="base.user_root" />-->
<!--            <field eval="False" name="doall" />-->
<!--            <field name="priority">1000</field>-->
            <field name="name">Cron Run All Masterdata</field>
            <field name="model_id" ref="model_ir_cron"/>
            <field name="state">code</field>
            <field name="code">model.run_manual_many_cron()</field>
            <field name="active" eval="False"/>
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=7)).strftime('%Y-%m-%d %H:%M:%S')" />
        </record>
    </data>
</openerp>
