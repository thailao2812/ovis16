<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
         <!-- Security gate queue sequence format -->
        <record id="seq_security_gate_queue" model="ir.sequence">  
            <field name="name">Security Gate Queue Sequence</field>  
            <field name="code">ned.security.gate.queue.seq</field>  
            <field name="prefix">DR/%(y)s%(month)s%(day)s/</field>
            <field name="padding">4</field>
            <field eval="1" name="number_increment"/>  
        </record>  
        
        
        
    </data>
</odoo>
