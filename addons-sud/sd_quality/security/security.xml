<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="0">
        
	    <record id="module_category_quanlity_sucden" model="ir.module.category">
             <field name="name">Quality Sucden</field>
            <field name="sequence">1</field>
        </record>
        
	    <record id="group_kcs_user" model="res.groups">
            <field name="name">KCS User</field>
            <field name="category_id" ref="sd_quality.module_category_quanlity_sucden"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin')),(4, ref('base.user_root'))]"/>
        </record>
        
        <record id="group_kcs_manager" model="res.groups">
            <field name="name">KCS Manager</field>
            <field name="category_id" ref="sd_quality.module_category_quanlity_sucden"/>
            <field name="implied_ids" eval="[(4, ref('sd_quality.group_kcs_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin')),(4, ref('base.user_root'))]"/>
        </record>
        
        <record id="warehouse_request_kcs_line_user_rule" model="ir.rule">
	        <field name="name">warehouse request kcs line user rule</field>
	        <field name="model_id" ref="model_request_kcs_line"/>
	        <field name="domain_force">[('picking_id.warehouse_id','in',user._warehouses_domain()),('picking_id.picking_type_id','in',user._picking_type_domain())]</field>
	        <field name="groups" eval="[(4, ref('sd_quality.group_kcs_user'))]"/>
	    </record>
	    
	    
	</data>
</odoo>