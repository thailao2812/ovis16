		<record id="stock_balancesheet_product_report" model="ir.actions.report">
	        <field name="name">Balance Sheet By Products</field>
	        <field name="type">ir.actions.report</field>
	        <field name="model">report.stock.balance.sheet</field>
	        <field name="binding_model_id" ref="sd_inventory.model_report_stock_balance_sheet"/>
