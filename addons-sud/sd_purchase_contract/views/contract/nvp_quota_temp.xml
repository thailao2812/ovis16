        <record model="ir.actions.act_window" id="action_purchase_contract_quota_temp">
            <field name="name">NVP Quota Temp</field>
            <field name="res_model">purchase.contract</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('type','=','purchase'),('cert_type','=','normal'),('is_temp_quota', '=', True)]</field>
            <field name="context">{'default_type':'consign'}</field>
        	<field name="search_view_id" ref="purchase_contract_search"/>
            <field name="context">{'default_type':'purchase', 'default_cert_type': 'normal', 'default_is_temp_quota': True, }</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a contract NVP Quota Temp, the first step of a new NVP.
                </p><p>
                </p>
            </field>
        </record>
    	
