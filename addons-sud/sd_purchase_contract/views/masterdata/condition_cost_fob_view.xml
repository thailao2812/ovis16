<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	
    	<record id="action_cost_fob" model="ir.actions.act_window">
            <field name="name">Cost FOB To HCM</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">fob.value</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a period, the first step of a new Period.
                </p><p>
                </p>
            </field>
        </record>
        

        <record id="view_cost_fob_tree" model="ir.ui.view">
            <field name="name">view_cost_fob_tree</field>
            <field name="model">fob.value</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="effective_date"/>
                    <field name="end_date"/>
                    <field name="state"/>
                </tree>
             </field>
        </record>

        <record id="view_cost_fob_form" model="ir.ui.view">
            <field name="name">view_cost_fob_form</field>
            <field name="model">fob.value</field>
			<field name="arch" type="xml">
	        <form>
                <header>
                        <button string="Stop Configuration" type="object" name="action_stop_config" class="oe_highlight" states="run"/>
                        <field name="state" widget="statusbar"/>
                </header>
	        	<sheet>
                    <h1>
                        <field name="name" readonly="1"/>
                    </h1>
                    <group>
                        <group>
                            <field name="effective_date" required="1"/>
                            <field name="end_date" attrs="{'invisible': [('end_date', '=', False)], 'readonly': [('state', '=', 'stop')]}"/>
                            </group>
                        <group>
                        </group>
                    </group>
                    <notebook>
                        <page string="Cost FOB" name="cost_fob">
                            <field name="cost_fob_lines">
                                <tree editable="bottom">
                                    <field name="product_id" domain="[('default_code', '=', 'FAQ')]" attrs="{'readonly': [('delivery_to_location', '!=', False)]}"/>
                                    <field name="partner_code" attrs="{'readonly': [('delivery_to_location', '!=', False)], 'required': [('product_id', '!=', False)]}"/>
                                    <field name="delivery_to_location" attrs="{'readonly': [('product_id', '!=', False)]}"/>
                                    <field name="diff_delivery_to_location"/>
                                    <field name="cost_fob_hcm"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
				</sheet>
    		</form>
	      </field>
	    </record>

        

    </data>
</openerp>