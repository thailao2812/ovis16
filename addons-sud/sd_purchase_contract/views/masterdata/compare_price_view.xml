<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        <record id="action_compare_price" model="ir.actions.act_window">
            <field name="name">Compare Price</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">compare.price</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a period, the first step of a new Period.
                </p><p>
                </p>
            </field>
        </record>



        <record id="view_compare_price_tree" model="ir.ui.view">
            <field name="name">view_compare_price_tree</field>
            <field name="model">compare.price</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="mkt"/>
                    <field name="ice_eu"/>
                    <field name="g2_replacement"/>
                    <field name="faq_price"/>
                    <field name="fob_price"/>
                    <field name="grade_price"/>
                </tree>
             </field>
        </record>

        <record id="view_compare_price_form" model="ir.ui.view">
	      <field name="name">view_compare_price_form</field>
			<field name="model">compare.price</field>
			<field name="arch" type="xml">
	        <form>
	        	<sheet>
                    <group>
                        <group>
                            <field name="mkt" />
                            <field name="ice_eu"/>
                            <field name="g2_replacement"/>
					    </group>
                        <group>
                            <field name="faq_price" />
                            <field name="fob_price"/>
                            <field name="grade_price"/>
                        </group>
                    </group>
				</sheet>
    		</form>
	      </field>
	    </record>


    </data>
</openerp>