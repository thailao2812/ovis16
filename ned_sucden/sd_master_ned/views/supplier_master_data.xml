<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_supplier_master_form" model="ir.ui.view">
        <field name="name">Supplier Master</field>
        <field name="model">supplier.master.data</field>
        <field name="arch" type="xml">
             <form string="Supplier Master">
                 <sheet>
                    <group>
                        <field name="supplier_id"/>
                        <field name="upload_count"/>
                        <field name="last_upload"/>
                        <field name="status"/>
                    </group>
                     <notebook>
                         <page name="import_ids" string="All Import">
                             <field name="import_ids" context="{'partner_id': supplier_id}">
                                 <tree>
                                     <field name="import_date"/>
                                     <field name="purchase_no"/>
                                     <field name="total_data"/>
                                     <field name="status_check" widget="color" readonly="1"/>
                                 </tree>
                             </field>
                         </page>
                     </notebook>
                 </sheet>
                 <div class="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="activity_ids"/>
                        <field name="message_ids"/>
                    </div>

            </form>
        </field>
    </record>
    <record id="view_supplier_master_tree" model="ir.ui.view">
        <field name="name">Supplier Master</field>
        <field name="model">supplier.master.data</field>
        <field name="arch" type="xml">
             <tree string="Supplier Master">
                 <field name="name"/>
                 <field name="upload_count"/>
                 <field name="last_upload"/>
                 <field name="status" widget="color" readonly="1"/>
            </tree>
        </field>
    </record>


     <record id="action_supplier_master" model="ir.actions.act_window">
            <field name="name">Supplier Master</field>
            <field name="res_model">supplier.master.data</field>
            <field name="view_mode">tree,form</field>
        </record>
</odoo>