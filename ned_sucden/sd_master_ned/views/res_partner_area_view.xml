<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_res_partner_area_tree_inherit_sucden_ned" model="ir.ui.view">
            <field name="name">view_res_partner_area_tree_inherit_sucden_ned</field>
            <field name="model">res.partner.area</field>
            <field name="inherit_id" ref="contacts_area.view_res_partner_area_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='gshape_area']" position="replace">
                </xpath>
                <xpath expr="//field[@name='gshape_radius']" position="replace">
                </xpath>
                <xpath expr="//field[@name='gshape_type']" position="after">
                    <field name="import_id"/>
                    <field name="import_date"/>
                    <field name="create_uid"/>
                </xpath>
            </field>
    </record>

    <record id="view_res_partner_area_form_inherit_sucden_ned" model="ir.ui.view">
            <field name="name">view_res_partner_area_form_inherit_sucden_ned</field>
            <field name="model">res.partner.area</field>
            <field name="inherit_id" ref="contacts_area.view_res_partner_area_form"/>
            <field name="arch" type="xml">
                <xpath expr="//label[@for='gshape_area']" position="replace">
                </xpath>
                <xpath expr="//field[@name='gshape_area']" position="replace">
                </xpath>
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="import_id" readonly="1"/>
                    <field name="import_date"/>
                    <field name="create_uid"/>
                </xpath>
                <xpath expr="//form/sheet/group/group[2]/div[1]" position="replace">

                </xpath>
            </field>
    </record>
</odoo>