                <field name="flat_percent" avg="flat_percent"/>
                <field name="pb1_percent" avg="pb1_percent"/>
                <field name="pb2_percent" avg="pb2_percent"/>
                <field name="sleeve_6_up_percent" avg="sleeve_6_up_percent"/>
                <field name="sleeve_5_5_up_percent" avg="sleeve_5_5_up_percent"/>
                <field name="sleeve_5_5_down_percent" avg="sleeve_5_5_down_percent"/>
                <field name="sleeve_5_down_percent" avg="sleeve_5_down_percent"/>

                <field name="sleeve_5_up_percent" avg="sleeve_5_up_percent"/>
                <field name="unhulled_percent" avg="unhulled_percent"/>
                <field name="remaining_coffee_percent" avg="remaining_coffee_percent"/>
                <field name="blacks_percent" avg="blacks_percent"/>
                <field name="half_monsoon_percent" avg="half_monsoon_percent"/>
                <field name="good_beans_percent" avg="good_beans_percent"/>

                <field name="moisture_percent" avg="moisture_percent"/>
            </xpath>

        </field>
    </record>

    <record id="sd_quality.action_report_ned_stock_stack_tree" model="ir.actions.act_window">
        <field name="domain">[('product_id.hide_report','=',False), ('product_id.default_code', '!=', '16001')]</field>
        </record>
</odoo>