<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="grn_india_report" model="ir.actions.report">
	        <field name="name">GRN</field>
	        <field name="type">ir.actions.report</field>
	        <field name="model">stock.picking</field>
	        <field name="binding_model_id" ref="stock.model_stock_picking"/>
	        <field name="report_name">grn_india_report</field>
	        <field name="report_type">aeroo</field>
	        <field name="in_format">oo-odt</field>
	        <field name="out_format" model="report.mimetypes" search="[('code','=','oo-pdf'),('compatible_types','=','oo-odt')]"/>
	        <field name="preview">True</field>
	        <field name="parser_state">loc</field>
	        <field name="parser_loc">sd_india_inventory/report/report_grn_india_parse.py</field>
	        <field name="report_file">sd_india_inventory/report/report_grn_india.odt</field>
	        <field name="tml_source">file</field>
	        <field name="print_report_name">(object._get_printed_report_name())</field>
    </record>

