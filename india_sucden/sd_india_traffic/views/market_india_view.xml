<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="market_india_tree_view" model="ir.ui.view">
            <field name="name">market_india_tree_view</field>
            <field name="model">market.india</field>
            <field name="arch" type="xml">
                <tree string="Market" editable="top" default_order="id desc">
                    <field name="name"/>
                    <field name="rate"/>
                </tree>
            </field>
        </record>

        <record id="market_india_search_view" model="ir.ui.view">
            <field name="name">market_india_search_view</field>
            <field name="model">market.india</field>
            <field name="arch" type="xml">
                <search string="Market">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record id="market_india_act_window" model="ir.actions.act_window">
            <field name="name">Market</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">market.india</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new Market.
                </p>
            </field>
        </record>


    </data>
</odoo>