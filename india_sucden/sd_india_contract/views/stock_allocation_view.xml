<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="group_date_allocation_stock_allocation" model="res.groups">
        <field name="name">Date Allocation View</field>
    </record>
    <record id="stock_allocation_view_form" model="ir.ui.view">
        <field name="name">stock_allocation_view_form</field>
        <field name="model">stock.allocation</field>
        <field name="arch" type="xml">
            <form string="Stock Allocation">
                <sheet>
                    <group>
                        <group>
                            <field name="contract_id" readonly="1"/>
                            <field name="product_id" readonly="1"/>
                            <field name="create_date"/>
                            <field name="date_picking" readonly="1"/>
                            <field name="qty_contract" readonly="1"/>
                            <field name="date_contract" readonly="1"/>
                            <field name="warehouse_id" readonly="1"/>
                        </group>
                        <group>
                            <field name="type_contract" readonly="1"/>
                            <field name="picking_id" readonly="1"/>
                            <field name="grade_id" readonly="1"/>
                            <field name="partner_id" readonly="1"/>
                            <field name="qty_received" readonly="1"/>
                            <field name="qty_unreceived" readonly="1"/>
                            <field name="qty_allocation" readonly="1"/>
                            <field name="qty_received_net" readonly="1"/>
                            <field name="qty_allocation_net" readonly="1"/>
                            <field name="qty_unreceived_net" readonly="1"/>
                            <field name="state" readonly="1" widget="badge"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

