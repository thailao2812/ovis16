<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="contract_regular_india_report" model="ir.actions.report">
	        <field name="name">Regular Contract</field>
	        <field name="type">ir.actions.report</field>
	        <field name="model">purchase.contract</field>
	        <field name="binding_model_id" ref="sd_purchase_contract.model_purchase_contract"/>
	        <field name="report_name">contract_regular_india_report</field>
	        <field name="report_type">aeroo</field>
	        <field name="in_format">oo-odt</field>
	        <field name="out_format" model="report.mimetypes" search="[('code','=','oo-pdf'),('compatible_types','=','oo-odt')]"/>
	        <field name="preview">True</field>
	        <field name="parser_state">loc</field>
	        <field name="parser_loc">sd_india_contract/report/report_regular_contract_parse.py</field>
	        <field name="report_file">sd_india_contract/report/report_regular_contract.odt</field>
	        <field name="tml_source">file</field>
	        <field name="print_report_name">(object._get_printed_report_name())</field>
    </record>

