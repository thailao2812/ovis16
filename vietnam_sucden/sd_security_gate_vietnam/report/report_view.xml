	        <field name="parser_loc">sd_security_gate_vietnam/report/printout_payment_npv_fix_npe_parser.py</field>
	        <field name="report_file">sd_security_gate_vietnam/report/printout_payment_npv_fix_npe.ods</field>
	        <field name="tml_source">file</field>
	        <field name="print_report_name">(object._get_printed_report_name())</field>
	    </record>

