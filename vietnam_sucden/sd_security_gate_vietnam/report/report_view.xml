	<record id="printout_payment_npv_fixed_by_npe_report" model="ir.actions.report">
	        <field name="name">Request Payment NVP fixed by NPE</field>
	        <field name="type">ir.actions.report</field>
	        <field name="model">request.payment</field>
	        <field name="binding_model_id" ref="sd_purchase_contract.model_request_payment"/>
	        <field name="report_name">printout_payment_npv_fixed_by_npe_report</field>
	        <field name="report_type">aeroo</field>
