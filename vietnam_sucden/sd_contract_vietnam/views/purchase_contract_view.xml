<odoo>
    <record id="view_purchase_contract_form_inherit_vietnam" model="ir.ui.view">
        <field name="name">view_purchase_contract_form_inherit_vietnam</field>
        <field name="model">purchase.contract</field>
        <field name="inherit_id" ref="sd_purchase_contract.view_purchase_contract_form"/>
        <field name="arch" type="xml">
			 <xpath expr="//page[@name='information']" position="after">
            	<page name="farmer" string="Farmer Information">
                    <field name="list_farmer" readonly="1">
                        <tree editable="1" limit="100">
                            <field name="date"/>
                            <field name="farmer_id"/>
                            <field name="farmer_code"/>
                            <field name="quantity" sum="quantity"/>
                        </tree>
                    </field>

                </page>
            </xpath>
            <xpath expr="//field[@name='contract_line']//tree//field[@name='difams']" position="after">
                <field name="equiv_faq_price"/>
            </xpath>
            <xpath expr="//field[@name='premium']" position="after">
                <field name="deposit_amount"/>
                <field name="print_company"/>
            </xpath>
<!--            <xpath expr="//field[@name='request_payment_ids']//form//field[@name='payment_quantity']" position="after">-->
