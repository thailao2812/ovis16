    <record id="view_ptbf_contract_form_inherit_vietnam" model="ir.ui.view">
        <field name="name">view_ptbf_contract_form_inherit_vietnam</field>
        <field name="model">purchase.contract</field>
        <field name="inherit_id" ref="sd_purchase_contract.view_ptbf_contract_form"/>
        <field name="arch" type="xml">
			 <xpath expr="//page[@name='information']" position="after">
            	<page name="farmer" string="Farmer Information">
                    <field name="list_farmer" readonly="1">
                        <tree editable="1" limit="100">
                            <field name="date"/>
                            <field name="farmer_id"/>
                            <field name="farmer_code"/>
                            <field name="quantity" sum="quantity"/>
                        </tree>
                    </field>

                </page>
            </xpath>
            <xpath expr="//field[@name='contract_line']//tree//field[@name='difams']" position="after">
                <field name="equiv_faq_price"/>
            </xpath>

            <xpath expr="//field[@name='ptbf_ids']//form//field[@name='diff']" position="after">
                <field name="contract_p_id"/>
            </xpath>
            <xpath expr="//field[@name='ptbf_ids']//tree//field[@name='date_fix']" position="after">
                <field name="contract_p_id"/>
            </xpath>
            <xpath expr="//field[@name='premium']" position="after">
                <field name="print_company"/>
            </xpath>
            <xpath expr="//field[@name='premium']" position="before">
                <field name="percent_advance_price"/>
            </xpath>

        </field>
    </record>

    <record id="view_consignment_agreement_form_inherit_vietnam" model="ir.ui.view">
        <field name="name">view_consignment_agreement_form_inherit_vietnam</field>
        <field name="model">purchase.contract</field>
        <field name="inherit_id" ref="sd_purchase_contract.view_consignment_agreement_form"/>
        <field name="arch" type="xml">
			 <xpath expr="//page[@name='information']" position="after">
            	<page name="farmer" string="Farmer Information">
                    <field name="list_farmer" readonly="1">
                        <tree editable="1" limit="100">
                            <field name="date"/>
                            <field name="farmer_id"/>
                            <field name="farmer_code"/>
                            <field name="quantity" sum="quantity"/>
                        </tree>
                    </field>

                </page>
            </xpath>
            <xpath expr="//field[@name='interest']" position="attributes">
                <attribute name="invisible">0</attribute>
            </xpath>
            <xpath expr="//field[@name='premium']" position="after">
                <field name="print_company"/>
            </xpath>

        </field>
    </record>

    <record id="view_nvp_contract_allocation_invoiced_tree_inherit_vietnam" model="ir.ui.view">
        <field name="name">view_nvp_contract_allocation_invoiced_tree_inherit_vietnam</field>
        <field name="model">purchase.contract</field>
        <field name="inherit_id" ref="sd_purchase_contract.view_nvp_contract_allocation_invoiced_tree"/>
        <field name="arch" type="xml">
			 <xpath expr="//field[@name='deadline_date']" position="after">
                 <field name="equiv_faq_price"/>
            </xpath>

        </field>
    </record>
    <record id="view_purchase_contract_tree_inherit_vietnam" model="ir.ui.view">
        <field name="name">view_purchase_contract_tree_inherit_vietnam</field>
        <field name="model">purchase.contract</field>
        <field name="inherit_id" ref="sd_purchase_contract.view_purchase_contract_tree"/>
        <field name="arch" type="xml">
			 <xpath expr="//field[@name='product_id']" position="after">
                 <field name="interest"/>
            </xpath>

        </field>
    </record>
</odoo>