<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_sale_order_quotations_google_map" model="ir.ui.view">
        <field name="name">view.sale.order.quotations.google_map</field>
        <field name="model">sale.order</field>
        <field name="priority" eval="100"/>
        <field name="arch" type="xml">
            <google_map js_class="google_map_sales" disable_area_selector="1" string="Quotations" lat="partner_latitude" lng="partner_longitude" marker_icon="circle" icon_scale="0.6" color="#ad2df7">
                <field name="partner_latitude"/>
                <field name="partner_longitude"/>
                <field name="name"/>
                <field name="partner_id"/>
                <field name="amount_total"/>
                <field name="currency_id"/>
                <field name="partner_contact_address"/>
            </google_map>
        </field>
    </record>
    <record id="sale.action_quotations_with_onboarding" model="ir.actions.act_window">
        <field name="view_mode">tree,kanban,form,calendar,pivot,graph,activity,google_map</field>
    </record>
